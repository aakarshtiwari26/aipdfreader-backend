{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/upload", "methods": ["POST"], "dest": "api/upload.js" },
    { "src": "/api/ask", "methods": ["POST"], "dest": "api/ask.js" },
    { "src": "/", "methods": ["GET"], "dest": "api/index.js" }
  ]
}
